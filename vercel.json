{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/public/(.*)",
            "destination": "/public/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/src/index.ts"
        }
    ]
}

